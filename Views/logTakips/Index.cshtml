@model IEnumerable<DepoStok.Models.logTakip>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.tabloAdi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.islemTipi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.islemTarihi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.detay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.kullanici)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.tabloAdi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.islemTipi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.islemTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.detay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kullanici.adSoyad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.islemId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.islemId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.islemId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
